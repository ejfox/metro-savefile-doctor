{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-copy-file",
    "fs:write-files",
    "fs:read-files",
    "fs:allow-write-file",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
